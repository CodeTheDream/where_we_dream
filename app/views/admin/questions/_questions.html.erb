<tbody id="questions">
  <% @questions.each do |question| %>
    <tr>
      <td><%= question.value %></td>
      <td><%= link_to 'Edit', edit_admin_question_path(question) %></td>
      <td><%= button_to 'Destroy', admin_question_path(question), method: :delete, remote: true %></td>
    </tr>
  <% end %>
</tbody>
