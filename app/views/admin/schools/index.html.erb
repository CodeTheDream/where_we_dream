<div id='body'>
  <p id='notice'><%= notice %></p>

  <h1>All Schools</h1>
  <h2>All Schools</h2>
  <h3>All Schools</h3>
  <h4>All Schools</h4>
  <h5>All Schools</h5>
  <h6>All Schools</h6>

  <table>
    <thead>
      <tr>
        <th>Name</th>
        <th>Rating</th>
        <th colspan='3'></th>
      </tr>
    </thead>

    <tbody>
      <% @schools.each do |school| %>
        <tr>
          <td><%= school.name %></td>
          <td><%= school.rating %></td>
          <td><%= link_to 'Show', admin_school_path(school) %></td>
          <td><%= link_to 'Edit', edit_admin_school_path(school) %></td>
          <td><%= link_to 'Destroy', admin_school_path(school), method: :delete, data: { confirm: 'Are you sure?' } %></td>
        </tr>
      <% end %>
    </tbody>
  </table>

  <br>

  <%= link_to 'New School', new_admin_school_path %>
  <%= link_to 'Back', root_path %>
</div>
